{
  "name": "将用户列表转excel",
  "sheets": [
    {
      "sheet_index": 0,
      "title_height": 48,
      "desc": "16*行数",
      "title_style": {
        "font": {
          "family": "宋体",
          "size": 12,
          "color": "#ffffff"
        },
        "fill": {
          "type": "pattern",
          "color": [
            "#3366FF"
          ],
          "pattern": 1
        },
        "alignment": {
          "vertical": "center",
          "WrapText": true
        }
      },
      "name_style": {
        "font": {
          "family": "Arial",
          "size": 10,
          "color": "#ffffff"
        },
        "fill": {
          "type": "pattern",
          "color": [
            "#0066CC"
          ],
          "pattern": 1
        },
        "alignment": {
          "vertical": "center"
        }
      },
      "content_style": {
        "NumFmt": 49
      },
      "title": "花名册\r\n1.学号/工号 是作为系统的唯一标志\r\n2.级别有excellent（优秀），good（良好），passed（合格）",
      "data": "[user_list]",
      "fields": [
        {
          "field": "[username]",
          "enable": false,
          "name": "学号",
          "width": 10
        },
        {
          "field": "[student_name]",
          "name": "学生",
          "width": 10
        },
        {
          "field": "[group_name]",
          "name": "小组",
          "width": 10
        },
        {
          "field": "[sport_level]",
          "name": "级别",
          "width": 10
        },
        {
          "field": "[before_score]",
          "name": "*课前得分",
          "width": 20
        },
        {
          "field": "[in_score]",
          "name": "*课中得分",
          "width": 20
        },

        {
          "field": "[after_score]",
          "name": "*课后得分",
          "width": 20
        }
      ]
    }
  ]
}